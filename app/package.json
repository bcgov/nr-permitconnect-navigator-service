{
  "name": "nr-permitting-navigator-service-app",
  "version": "0.8.0",
  "license": "Apache-2.0",
  "private": true,
  "description": "Permit Connect Services",
  "author": "NRM Permitting and Data Solutions <NRM.PermittingAndData@gov.bc.ca>",
  "scripts": {
    "build:all": "tsx ./deploy-utils.ts",
    "build": "tsc --project tsconfig.build.json",
    "clean:all": "tsx ./deploy-utils.ts clean",
    "clean": "rimraf coverage dist sbin",
    "format": "prettier ./src --write",
    "lint": "eslint . --no-fix --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint . --fix --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "migrate:down": "knex migrate:down",
    "migrate:latest": "knex migrate:latest",
    "migrate:list": "knex migrate:list",
    "migrate:make": "knex migrate:make",
    "migrate:rollback:all": "knex migrate:rollback --all",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:up": "knex migrate:up",
    "migrate": "knex migrate:latest --knexfile ./sbin/knexfile.js",
    "postbuild:all": "npm run build",
    "postbuild": "tsx ./deploy-utils.ts docs",
    "postclean:all": "npm run clean",
    "postprisma:case": "npm run prisma:sync",
    "postprisma:migrate:rollback": "npm run prisma:sync",
    "postprisma:migrate:down": "npm run prisma:sync",
    "postprisma:migrate:up": "npm run prisma:sync",
    "postprisma:migrate:dev": "npm run seed:dev",
    "postprisma:migrate": "npm run prisma:case",
    "postprisma:sync": "npm run prisma:generate",
    "postrebuild": "npm run build",
    "postreinstall": "npm install",
    "postseed:dev": "npm run prisma:sync",
    "posttest": "tsx ./lcov-fix.ts",
    "prelint": "npm run typecheck",
    "prelint:fix": "npm run typecheck",
    "pretest": "npm run lint",
    "prisma:case": "prisma-case-format -f \"./src/db/prisma/schema.prisma\" --table-case=snake --field-case=camel --enum-case=snake",
    "prisma:generate": "prisma generate",
    "prisma:migrate:rollback": "npm run migrate:rollback",
    "prisma:migrate:down": "npm run migrate:down",
    "prisma:migrate:up": "npm run migrate:up",
    "prisma:migrate:dev": "npm run migrate:latest",
    "prisma:migrate": "npm run migrate:latest",
    "prisma:sync": "prisma db pull",
    "purge": "rimraf node_modules",
    "rebuild": "npm run clean",
    "reinstall": "npm run purge",
    "seed:make": "knex seed:make",
    "seed:dev": "knex seed:run --specific=dev_groups.ts",
    "seed": "knex seed:run",
    "serve": "tsx watch --include \"./config/*\" --include \"./dist/*\" ./server --clear-screen=false",
    "start": "tsx ./server",
    "test": "jest --verbose --forceExit --detectOpenHandles",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^6.16.1",
    "@types/proj4": "^2.19.0",
    "axios": "^1.12.2",
    "compression": "^1.8.1",
    "config": "^4.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "express-winston": "^4.2.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "pg": "^8.16.3",
    "proj4": "^2.19.10",
    "tsx": "^4.20.5",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-transport": "^4.9.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/compression": "^1.8.1",
    "@types/config": "^3.3.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-serve-static-core": "^5.0.7",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.1",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.3",
    "jest-mock-extended": "^4.0.0",
    "prettier": "^3.6.2",
    "prisma": "^6.16.1",
    "prisma-case-format": "^2.2.1",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "prisma": {
    "schema": "src/db/prisma/schema.prisma"
  }
}
